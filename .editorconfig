# top-most EditorConfig file
root = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,py}]
charset = utf-8

# Indentation override for all JS
[*.js]
indent_style = tab
indent_size = 2

# Matches the exact files either package.json
[{package.json}]
indent_style = tab
indent_size = 2